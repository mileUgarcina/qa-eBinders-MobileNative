<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Set - Settings Page">

<!--Device Parameter-->
<!--    <parameter name="deviceParameter" value="localiOSimulatorSCapsWeb"/>-->
    <parameter name="deviceParameter" value="SauceLabs_iOS_RealDevice_App"/>
<!--    <parameter name="deviceParameter" value="BrowserStack_iOS_RealDevice_App"/>-->
    <parameter name="platformParameter" value="iOS"/>
    <parameter name="browserName" value="Safari"/>
<!--TC Parameter-->
    <parameter name="user" value="Participant"/>
    <parameter name="crete_Xray_Test_Run" value="false"/>

    <test name="Settings Page">
        <classes>
            <class name="testing.appium.eBinders.Settings_Page">

                <methods>
                    <include name="mobile_Settings_Page_UAT_Symbol"/>
                    <include name="mobile_Settings_Page_Account"/>
                    <include name="mobile_Settings_Page_App_Version"/>
                    <include name="mobile_Settings_Page_Contact_Support"/>
                    <include name="mobile_Settings_Page_Privacy_Policy"/>
                    <include name="mobile_Settings_Page_Software_Licenses"/>
                    <include name="mobile_Settings_Page_Sign_Out"/>
                </methods>

            </class>
        </classes>
    </test>
</suite>


